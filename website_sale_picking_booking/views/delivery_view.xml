<?xml version="1.0" encoding="utf-8" ?>
<odoo>
     <record id="view_delivery_carrier_form_with_onsite_picking" model="ir.ui.view">
         <field name="inherit_id" ref="delivery.view_delivery_carrier_form"/>
         <field name="name">Delivery Carrier with Onsite Picking disponilibity</field>
         <field name="model">delivery.carrier</field>
         <field name="arch" type="xml">
             <xpath expr="//notebook" position="inside">
                <page string="Booking disponnibility">
                    <group>
                        <field name="resource_calendar_id"/>
                        <field name="available_picking_delay" widget="float_time"/>
                        <field name="slot_duration" widget="float_time"/>
                    </group>
                </page>
             </xpath>
         </field>
    </record>
</odoo>
